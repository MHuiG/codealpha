{
  "name": "codealpha",
  "displayName": "CodeAlpha",
  "description": "Code Alpha",
  "version": "0.0.2",
  "publisher": "MHuiG",
  "repository": "https://github.com/MHuiG/codealpha",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.68.0"
  },
  "license": "GPLv3",
  "keywords": [
		"CodeAlpha",
		"Code Alpha"
	],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codealpha.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codealpha.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:package": "vsce package",
    "vscode:publish": "vsce publish",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.68.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "@vscode/test-electron": "^2.1.3"
  }
}